# Server Configuration
NODE_ENV=development
PORT=3000
API_BASE_URL=http://localhost:3000

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key

# Google Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-2.0-flash-exp

# WhatsApp Configuration (AiSensy/Twilio)
WHATSAPP_API_URL=https://backend.aisensy.com/campaign/t1/api/v2
WHATSAPP_API_KEY=your_aisensy_api_key
WHATSAPP_PHONE_NUMBER=your_whatsapp_business_number

# Security
WEBHOOK_VERIFY_TOKEN=your_secure_random_token_here
JWT_SECRET=your_jwt_secret_for_dashboard_auth

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info